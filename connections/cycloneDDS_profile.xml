<CycloneDDS xmlns="https://cdds.io/config">
  <Domain id="any">
    <General>
      <NetworkInterfaceAddress>eth1</NetworkInterfaceAddress>
      <AllowMulticast>true</AllowMulticast> 
    </General>
    <Tracing>
        <Verbosity>config</Verbosity>
        <OutputFile>stdout</OutputFile>
    </Tracing>
  </Domain>
</CycloneDDS>